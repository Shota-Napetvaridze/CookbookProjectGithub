<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="34.0" prefWidth="134.0" style="-fx-background-color: ffa9a9;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TagController">
   <children>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="134.0" spacing="5.0">
         <children>
            <Button fx:id="tagButton" mnemonicParsing="false" onMousePressed="#tagClick" prefHeight="26.0" prefWidth="23.0" style="-fx-border-color: #000000;">
               <HBox.margin>
                  <Insets left="2.0" />
               </HBox.margin>
            </Button>
            <Label fx:id="tagLbl" prefHeight="24.0" prefWidth="78.0" text="Vegan" />
         </children>
      </HBox>
   </children>
</AnchorPane>
