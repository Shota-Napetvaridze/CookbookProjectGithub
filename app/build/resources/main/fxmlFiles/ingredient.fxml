<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="34.0" prefWidth="134.0" style="-fx-background-color: ffa9a9;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.IngredientController">
   <children>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="134.0" spacing="5.0">
         <children>
            <Button fx:id="ingredientButton" mnemonicParsing="false" onMousePressed="#ingredientClick" prefHeight="26.0" prefWidth="38.0" style="-fx-border-color: #000000;">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Label fx:id="ingredientLbl" prefHeight="24.0" prefWidth="108.0" text="Beef" />
         </children>
      </HBox>
   </children>
</AnchorPane>
